                    data-bs-toggle="collapse"
                    data-bs-target="#navbarTogglerDemo01"
                    aria-controls="navbarTogglerDemo01" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
                    <ul class="navbar-nav ms-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="/wProfile"><%=
                                    profileName %></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/parts&services">Show
                                    All</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/create_appointment">Create
                                    Appointments</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/search_services">Search
                                    Services</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/search_parts">Search
                                    Parts</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/logout">Logout</a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href=""
                                    role="button"
                                    data-bs-toggle="dropdown"
                                    aria-expanded="false">More</a>
                                <ul class="dropdown-menu">
                                    <li><a class="dropdown-item"
                                            href="About-Us.html">About Us</a></li>
                                    <li><a class="dropdown-item"
                                            href="About-Us.html">About Us</a></li>
                                    <!--  <li><a class="dropdown-item" href="">FAQ</a></li> -->
                                    <li><a class="dropdown-item"
                                            href="Contact.html">Contact Us</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </nav>
                <h1 class="text-center mt-3 mb-3">Parts</h1>
                <div class="container-sm">


                    <div class="row">

                        <br>
                        <br>
                        <hr>
                        <br>
                        <br>
                        <div>
                            <div class="card">
                                <div class="card-header">
                                    <div class="row">
                                        <div class="col"></div>
                                        <div class="col"></div>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <div
                                        class="table-responsive">
                                        <table class="table
                                            table-bordered
                                            problem-table">
                                            <tr>
                                                <th>Parts ID</th>
                                                <th>Manufacturer</th>
                                                <th>Part Name</th>
                                                <th>Price</th>
                                                <th>Quantity</th>
                                                <th>Details</th>
                                            </tr>
                                            <tr>
                                                <% for(let i=0; i<parts.length;
                                                    i++){ %>
                                                    <tr>
                                                        <td><%=parts[i].parts_id
                                                                %></td>
                                                            <td><%=parts[i].manufacturer
                                                                    %></td>
                                                                <td><%=parts[i].p_name
                                                                        %></td>
                                                                    <td><%=parts[i].price
                                                                            %></td>
                                                                        <td><%=parts[i].quantity
                                                                                %></td>
                                                                            <td><%=parts[i].details
                                                                                    %></td>
                                                                            </tr>
                                                                            <% }
                                                                                %>
                                                                            </tr>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>

                                                    </div>

                                                </div>
                                                <h1 class="text-center mt-3
                                                    mb-3">Services</h1>
                                                <div class="container-sm">
                                                    <div class="row">

                                                        <br>
                                                        <br>
                                                        <hr>
                                                        <br>
                                                        <br>
                                                        <div>
                                                            <div
                                                                class="card">
                                                                <div
                                                                    class="card-header">
                                                                    <div
                                                                        class="row">
                                                                        <div
                                                                            class="col"></div>
                                                                        <div
                                                                            class="col"></div>
                                                                    </div>
                                                                </div>
                                                                <div
                                                                    class="card-body">
                                                                    <div
                                                                        class="table-responsive">
                                                                        <table
                                                                            class="table
                                                                            table-bordered
                                                                            problem-table">
                                                                            <tr>
                                                                                <th>Service
                                                                                    ID</th>
                                                                                <th>Service
                                                                                    Name</th>
                                                                                <th>Price</th>
                                                                                <th>Details</th>
                                                                                <th>Availability</th>
                                                                            </tr>
                                                                            <tr>
                                                                                <%
                                                                                    for(let
                                                                                    i=0;
                                                                                    i<services.length;
                                                                                    i++){
                                                                                    %>
                                                                                    <tr>
                                                                                        <td><%=services[i].service_id
                                                                                                %></td>
                                                                                            <td><%=services[i].s_name
                                                                                                    %></td>
                                                                                                <td><%=services[i].price
                                                                                                        %></td>
                                                                                                    <td><%=services[i].details
                                                                                                            %></td>
                                                                                                        <td><%=services[i].availability
                                                                                                                %></td>
                                                                                                        </tr>
                                                                                                        <%
                                                                                                            }
                                                                                                            %>
                                                                                                        </table>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </body>
                                                                        </html>
