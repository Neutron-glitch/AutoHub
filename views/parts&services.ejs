    <body onload="sortOnLoad()">

        <section id="title">


            <!-- Nav Bar -->
            <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
                <a id="Knapsack" class="navbar-brand" style="padding: 0 15px;"
                    href="/uProfile">Autohub</a>
